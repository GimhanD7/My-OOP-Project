<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>newMemberAddServlet</servlet-name>
        <servlet-class>.newMemberAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Myservlet</servlet-name>
        <servlet-class>.Myservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateMemberDetails</servlet-name>
        <servlet-class>.UpdateMemberDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>memberServlet</servlet-name>
        <servlet-class>memberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalaryServlet</servlet-name>
        <servlet-class>.SalaryServletInsert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalaryServletDelete</servlet-name>
        <servlet-class>.SalaryServletDelete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalaryServletUpdate</servlet-name>
        <servlet-class>.SalaryServletUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteSrevlet</servlet-name>
        <servlet-class>deleteSrevlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>newMemberAddServlet</servlet-name>
        <url-pattern>/newMemberAddServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Myservlet</servlet-name>
        <url-pattern>/Myservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/ProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateMemberDetails</servlet-name>
        <url-pattern>/UpdateMemberDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>memberServlet</servlet-name>
        <url-pattern>/memberServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalaryServlet</servlet-name>
        <url-pattern>/SalaryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalaryServletDelete</servlet-name>
        <url-pattern>/SalaryServletDelete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalaryServletUpdate</servlet-name>
        <url-pattern>/SalaryServletUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteSrevlet</servlet-name>
        <url-pattern>/deleteSrevlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
